{
  "description": "五子棋小程序生产环境配置",
  "packOptions": {
    "ignore": [
      {
        "type": "file",
        "value": ".eslintrc.js"
      },
      {
        "type": "file", 
        "value": "project.config.json"
      },
      {
        "type": "file",
        "value": "README.md"
      }
